 pastoUm = {'vacas':['branca','preta','bege','marrom'],
            'bezerro':['branca','preta','bege','marrom'],
            'cabra':['branca','preta','bege','marrom'],
            'ovelha':['branca','preta','bege','marrom']}
 pastoDois = []
 print(f'O pasto um est치 composto por:{pastoUm}')
 print('O pasto dois est치 vazio')
 print('Movendo os animais de pasto...')
 pastoDois.append(pastoUm['vacas'])# pastoDois.append(pastoUm['bezerro'])
 pastoDois.append(pastoUm['cabra'])
 pastoDois = pastoUm.pop('vacas')
 pastoDois = pastoUm.pop('bezerro')
 pastoDois = pastoUm.pop('cabra')
 print(f'O pasto um agora est치 composto por:\n{pastoUm}')
 print(f'E o pasto dois agora est치 composto por:\n vacas: {pastoDois}\n bezerro: {pastoDois}\n cabra: {pastoDois}')
